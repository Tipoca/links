# A test of client stubs / server stubs called from the other side.

fun f() client {
  alertDialog(server_concat("the concatenation", " did succeed"))
}

fun g() server { 
  debug("hello, server's g here.");
  alertDialog("this is the server talking.")
}

page
  <html>
    <p><a href="#" l:onclick="{f()}">Run concat on server</a></p>
    <a href="#" l:onclick="{g()}">Run client's alertDialog from server</a>
  </html>
